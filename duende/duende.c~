#include <stdio.h>
#include <stdlib.h>
#define M 10

int G[M][M];

int main(){
	
	int n = 0, m = 0, i = 0, j = 0, read = 0;
	
	scanf("%d %d", &n, &m);

	for(i = 0; i < n; i++){
		for(j = 0; j < m; j++){
			scanf("%d", &read);
			
			
			
			if(read > 0)
				read = 1;
			
			G[i][j] = read;
		}
	}

	return 0;
}
