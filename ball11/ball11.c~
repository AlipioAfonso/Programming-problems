#include <stdio.h>
#define MAX 1005

int N = 0, m[MAX][MAX];

/*inline int max(int a, int b, int c){
	return (a > b && a > c) ? a : ((b > a && b > c) ? b : c);
}*/

inline int max(int a, int b){
	return a > b ? a : b;
}

int main(){
	int n = 0, i = 0, j = 0, maior = 0, col = 0;

	while(1){
		scanf("%d", &N);
		getchar();

		if(!N)
			break;

		for(i = 0; i < MAX; i++)
			for(j = 0; j < MAX; j++)
				m[i][j] = 0;

		maior = 0;
		
		N++;
		
		for(i = 2; i <= N; i++){
			for(j = 1; j < i; j++){
				scanf("%d", &m[i][j]);
				getchar();
				m[i][j] += m[i - 1][j] + m[i - 1][j - 1] - m[i - 2][j - 1];
				maior = max(maior, m[i][j]);
			}
		}
		
		for(i = 2; i <= N; i++){
			for(j = 1; j < i; j++)
				printf("%d ", m[i][j]);
			printf("\n");
		}
		
		//printf("%d\n", maior);
	}

	return 0;
}
